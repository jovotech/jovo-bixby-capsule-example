result-view {
  match {
    JovoResponse (response)
  }

  message {
    template("#{value (response._JOVO_SPEECH_)}")
  }

  render {
    layout {
      section {
        content {
          single-line {
            text {
              style (Detail_M)
              value ("#{value (response._JOVO_LAYOUT_.single)}")
            }
          }
        }
      }
    }
  }
}